{
  "name": "@defra/forms-runner",
  "version": "3.0.0",
  "description": "Defra forms runner",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "check-forms": "node bin/run/check",
    "watch": "babel --watch --extensions \".ts\" --ignore \"**/*.test.*\" src -d dist --copy-files -s inline",
    "start": "node dist/index.js",
    "start:local": "NODE_ENV=development nodemon dist/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run start:local\"",
    "build": "npm run build:css && npm run babel-build && npm run check-forms",
    "build:css": "sass src/client/sass:public/build/stylesheets -s compressed -I . --no-source-map --quiet-deps",
    "babel-build": "babel --extensions \".ts\" --ignore \"**/*.test.*\" src -d dist --copy-files -s inline",
    "format": "prettier --write \"**/*.{cjs,js,json,md,mjs,scss,ts}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md,mjs,scss,ts}\"",
    "lint": "npm run lint:js && npm run lint:types",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "lint:types": "tsc --noEmit",
    "test": "jest --color --coverage --verbose",
    "test:watch": "jest --color --watch",
    "a11y": "node test/audit/components && node lighthouse",
    "symlink-env": "./bin/symlink-config",
    "generate-docs": "typedoc src --tsconfig tsconfig.json --out ./docs/typedoc --exclude \"**/*+(index|.test|.jest).ts\"",
    "prepare": "husky || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XGovFormBuilder/digital-form-builder/tree/main/builder"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@defra/forms-model": "^3.0.2",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/bell": "^13.0.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.3.9",
    "@hapi/hoek": "^11.0.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/scooter": "^7.0.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "@hapipal/schmervice": "^3.0.0",
    "accept-language-parser": "^1.5.0",
    "accessible-autocomplete": "^2.0.4",
    "atob": "^2.1.2",
    "aws-sdk": "^2.1599.0",
    "blankie": "^5.0.0",
    "blipp": "^4.0.2",
    "boom": "^7.3.0",
    "btoa": "^1.2.1",
    "config": "^3.3.11",
    "date-fns": "^3.6.0",
    "expr-eval": "^2.0.2",
    "form-data": "^4.0.0",
    "govuk-frontend": "^4.8.0",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "hapi-rate-limit": "^7.1.0",
    "hmpo-components": "^5.12.0",
    "hoek": "^6.1.3",
    "ioredis": "^4.28.5",
    "joi": "^17.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "nodemailer": "^6.9.13",
    "notifications-node-client": "^8.0.0",
    "nunjucks": "^3.2.4",
    "pg-boss": "^9.0.3",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0",
    "querystring": "^0.2.0",
    "resolve": "^1.22.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/eslint-plugin": "^7.23.5",
    "@babel/plugin-syntax-import-attributes": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@oclif/core": "^1.26.2",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cookie": "^0.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "sass": "^1.75.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "@types/hapi": "^18.0.14",
    "@types/hapi__yar": "^10.1.6",
    "@types/hoek": "^4.1.7",
    "@types/jest": "^29.5.12",
    "@types/mysql": "^2.15.26",
