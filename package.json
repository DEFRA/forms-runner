{
  "name": "@defra/forms-runner",
  "version": "3.0.0",
  "description": "Defra forms runner",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "postinstall": "bash install_model.sh",
    "check-forms": "node bin/run/check",
    "watch": "babel --watch --extensions '.ts' --ignore '**/*.test.js' src -d dist --copy-files -s inline",
    "start": "node dist/index.js",
    "start:local": "NODE_ENV=development nodemon dist/index.js",
    "dev": "concurrently 'npm run watch' 'npm run start:local'",
    "build": "npm run build:css && npm run babel-build && npm run check-forms",
    "build:css": "sass src/client/sass:public/build/stylesheets -s compressed -I . --no-source-map --quiet-deps",
    "babel-build": "babel --extensions '.ts' --ignore '**/*.test.js' src -d dist --copy-files -s inline",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "npm run lint && npm run type-check && npm run unit-test",
    "test-cov": "npm run unit-test-cov",
    "test:dev": "NODE_ENV=test lab -T test/.transform.js -v test --coverage-exclude",
    "unit-test": "NODE_ENV=test lab -T test/.transform.js -v test -S -v -r console -o stdout -r html -o unit-test.html -I version -l",
    "unit-test-cov": "NODE_ENV=test lab -T test/.transform.js -v test -t 83 -S -v -r console -o stdout -r lcov -o test-coverage/lab/lcov.info -r html -o test-coverage/lab/unit-test.html -r junit -o test-results/junit/unit-test.xml -I version -l",
    "a11y": "node test/audit/components && node lighthouse",
    "symlink-env": "./bin/symlink-config",
    "type-check": "tsc --noEmit",
    "generate-docs": "typedoc src --tsconfig tsconfig.json --out ./docs/typedoc --exclude '**/*+(index|.test|.jest).ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XGovFormBuilder/digital-form-builder/tree/main/builder"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.4.0",
    "@hapi/bell": "^13.0.1",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/code": "^8.0.1",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^20.3.0",
    "@hapi/hoek": "^11.0.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/scooter": "^7.0.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.0.1",
    "@hapi/yar": "^11.0.1",
    "accept-language-parser": "^1.5.0",
    "accessible-autocomplete": "^2.0.2",
    "atob": "^2.1.2",
    "aws-sdk": "^2.1531.0",
    "blankie": "^5.0.0",
    "blipp": "^4.0.1",
    "boom": "^7.3.0",
    "btoa": "^1.2.1",
    "config": "^3.3.7",
    "date-fns": "^2.24.0",
    "expr-eval": "^2.0.2",
    "form-data": "^4.0.0",
    "govuk-frontend": "^4.3.1",
    "hapi-pino": "^8.0.0",
    "hapi-pulse": "^3.0.0",
    "hapi-rate-limit": "^4.1.0",
    "hmpo-components": "^5.2.1",
    "hoek": "^6.1.3",
    "ioredis": "^4.16.1",
    "joi": "^17.2.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.6.0",
    "notifications-node-client": "^7.0.4",
    "nunjucks": "^3.2.3",
    "pg-boss": "^9.0.3",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.3",
    "querystring": "^0.2.0",
    "resolve": "^1.19.0",
    "schmervice": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/eslint-plugin": "^7.23.5",
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
