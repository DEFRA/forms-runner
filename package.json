{
  "name": "@defra/forms-runner",
  "version": "3.0.0",
  "description": "Defra forms runner",
  "type": "module",
  "main": ".server/index.js",
  "private": true,
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "sass src/client/sass:public/build/stylesheets -s compressed -I ./node_modules --quiet-deps",
    "build:server": "babel --delete-dir-on-start --extensions \".ts\" --ignore \"**/*.test.ts\" --copy-files --no-copy-ignored --source-maps --out-dir ./.server ./src",
    "dev": "concurrently \"npm run client:watch\" \"npm run server:watch\"",
    "dev:debug": "concurrently \"npm run client:watch\" \"npm run server:debug\"",
    "format": "npm run format:check -- --write",
    "format:check": "prettier --cache --cache-location .cache/prettier --cache-strategy content --check \"**/*.{cjs,js,json,md,mjs,scss,ts}\"",
    "postinstall": "npm run setup:husky",
    "lint": "npm run lint:js && npm run lint:types",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore .",
    "lint:types": "tsc --noEmit",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "client:watch": "npm run build:client -- --watch",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=${NODE_ENV:-production} node --enable-source-maps ./.server",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "check-forms": "node bin/run/check/check.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XGovFormBuilder/digital-form-builder/tree/main/builder"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@defra/forms-model": "^3.0.84",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/bell": "^13.0.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.3.9",
    "@hapi/hoek": "^11.0.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/scooter": "^7.0.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "@hapipal/schmervice": "^3.0.0",
    "accept-language-parser": "^1.5.0",
    "accessible-autocomplete": "^2.0.2",
    "atob": "^2.1.2",
    "blankie": "^5.0.0",
    "blipp": "^4.0.2",
    "btoa": "^1.2.1",
    "config": "^3.3.7",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "expr-eval": "^2.0.2",
    "form-data": "^4.0.0",
    "govuk-frontend": "^4.8.0",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "hapi-rate-limit": "^7.1.0",
    "hmpo-components": "^6.5.0",
    "hoek": "^6.1.3",
    "ioredis": "^4.16.1",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "notifications-node-client": "^7.0.4",
    "nunjucks": "^3.2.3",
    "pg-boss": "^9.0.3",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "resolve": "^1.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-syntax-import-attributes": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@oclif/core": "^3.26.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
