    "cookie": "^0.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "prisma": "^5.1.1",
    "sass": "^1.49.9",
    "sinon": "^13.0.1",
    "typedoc": "^0.25.9",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@types/hapi": "^18.0.7",
