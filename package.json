    "@types/hoek": "^4.1.4",
    "@types/mysql": "^2.15.21",
    "@types/nodemailer": "^6.4.4",
    "@types/url-parse": "^1.4.8",
    "@types/wreck": "^14.0.0"
  },
  "pkg": {
    "assets": [
      "../node_modules/.prisma/client/*.node"
    ]
  }
}
