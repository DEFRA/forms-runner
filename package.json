    "@types/nodemailer": "^6.4.14",
    "@types/url-parse": "^1.4.11",
    "@types/wreck": "^14.0.4"
  },
  "pkg": {
    "assets": [
      "../node_modules/.prisma/client/*.node"
    ]
  }
}
